REM Usage : medInria-dev.bat
@echo off
set PATH=%PATH%;@dtk_ROOT@/bin/%1;@ITK_ROOT@/bin/%1;@TTK_ROOT@/bin/%1;@VTK_ROOT@/bin/%1;@QtDCM_ROOT@/bin/%1;@medInria_BINARY_DIR@/bin/%1;@QT_BINARY_DIR@
set MEDINRIA_PLUGINS_DIR=@medInria_BINARY_DIR@/bin/plugins/%1;@DEV_PLUGINS_DIRS@
set MEDINRIA_PLUGINS_DIR_LEGACY=@medInria_BINARY_DIR@/bin/plugins_legacy/%1:@DEV_PLUGINS_LEGACY_DIRS@

start devenv.exe @medInria_BINARY_DIR@/ALL_BUILD.vcxproj
exit
